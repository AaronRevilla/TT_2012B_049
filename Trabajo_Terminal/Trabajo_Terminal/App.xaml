<Application x:Class="GraphicNotes.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/BaseObject.xaml"/>
                <ResourceDictionary Source="Resources/Styles/TextObject.xaml"/>
                <ResourceDictionary Source="Resources/Styles/TableObject.xaml"/>
                <ResourceDictionary Source="Resources/Styles/Toolbox.xaml"/>
                <ResourceDictionary Source="Resources/Styles/DocumentPanel.xaml"/>
                <ResourceDictionary Source="Resources/Collections/ToolboxCollection.xaml"/>
                <ResourceDictionary Source="Languages/StringResources.xaml"/>

                <ResourceDictionary Source="Resources/Styles/MetroButton.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
