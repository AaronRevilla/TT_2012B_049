<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:w="clr-namespace:GraphicNotes.Views.Toolbox"
                    >
                    
    <w:Toolbox x:Key="ToolboxCollection"
               DefaultItemSize="70,72" ScrollViewer.VerticalScrollBarVisibility="Disabled">
        <ItemsControl.Items>
            <w:ToolboxObject Style="{StaticResource ToolboxObjectStyle}" 
                             Text="{DynamicResource Figure3D}"
                             Type="Figure3D" 
                             Source="/GraphicNotes;component/Images/ObjectIcons/figure.png" />
            <w:ToolboxObject Style="{StaticResource ToolboxObjectStyle}" 
                             Text="{DynamicResource Line}" 
                             Type="Line"
                             Source="/GraphicNotes;component/Images/ObjectIcons/line.png" />
            <w:ToolboxObject Style="{StaticResource ToolboxObjectStyle}" 
                             Text="{DynamicResource Picture}"  
                             Type="Picture" 
                             Source="/GraphicNotes;component/Images/ObjectIcons/picture.png" />
            <w:ToolboxObject Style="{StaticResource ToolboxObjectStyle}" 
                             Text="{DynamicResource Plot}"  
                             Type="Plot" 
                             Source="/GraphicNotes;component/Images/ObjectIcons/plot.png" />
            <w:ToolboxObject Style="{StaticResource ToolboxObjectStyle}" 
                             Text="{DynamicResource Polygon}"  
                             Type="Polygon" 
                             Source="/GraphicNotes;component/Images/ObjectIcons/polygon.png" />
            <w:ToolboxObject Style="{StaticResource ToolboxObjectStyle}" 
                             Text="{DynamicResource Table}"  
                             Type="Table" 
                             Source="/GraphicNotes;component/Images/ObjectIcons/table.png" />
            <w:ToolboxObject Style="{StaticResource ToolboxObjectStyle}" 
                             Text="{DynamicResource Text}"  
                             Type="Text" 
                             Source="/GraphicNotes;component/Images/ObjectIcons/text.png" />
            <w:ToolboxObject Style="{StaticResource ToolboxObjectStyle}" 
                             Text="{DynamicResource Video}"  
                             Type="Video" 
                             Source="/GraphicNotes;component/Images/ObjectIcons/video.png" />
        </ItemsControl.Items>
    </w:Toolbox>
</ResourceDictionary>